{
  "name": "svelte-play",
  "version": "1.0.0",
  "description": "A boilerplate for Svelte, Material, Babel, and PostCSS.",
  "engines": {
    "node": "10"
  },
  "scripts": {
    "start": "cross-env BABEL_ENV=app webpack-dev-server --progress",
    "build": "rimraf public &&  cross-env NODE_ENV=production BABEL_ENV=app webpack --env.prod",
    "lint": "eslint 'src/**/*.{js,svelte}' && stylelint 'src/**/*.{css,svelte}'",
    "unit": "cross-env BABEL_ENV=test JEST_ENV=app jest \"src/.*\\.js$\" --coverage --forceExit",
    "e2e": "cross-env BABEL_ENV=test JEST_ENV=e2e jest \"src/.*\\.js$\" --forceExit",
    "_codecov": "codecov",
    "_firebase": "firebase"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Shyam-Chen/Svelte-Play.git"
  },
  "author": "Shyam Chen",
  "license": "MIT",
  "dependencies": {
    "chart.js": "^2.8.0",
    "core-js": "^3.1.1",
    "svero": "^0.5.0"
  },
  "devDependencies": {
    "@fullhuman/postcss-purgecss": "1.2.0",
    "autoprefixer": "^9.6.0",
    "svelte": "^3.4.2",
    "@babel/core": "^7.4.4",
    "@babel/preset-env": "^7.4.4",
    "babel-eslint": "^10.0.1",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.0.6",
    "copy-webpack-plugin": "^5.0.3",
    "cross-env": "^5.2.0",
    "css-loader": "^2.1.1",
    "cssnano": "^4.1.10",
    "eslint": "^5.16.0",
    "eslint-config-airbnb-base": "^13.1.0",
    "eslint-import-resolver-webpack": "^0.11.1",
    "eslint-plugin-import": "^2.17.2",
    "html-webpack-plugin": "4.0.0-alpha",
    "file-loader": "^3.0.1",
    "jest": "^24.8.0",
    "jest-transform-svelte": "^2.0.0",
    "postcss-import": "^12.0.1",
    "postcss-loader": "^3.0.0",
    "postcss-preset-env": "^6.6.0",
    "process-envify": "^0.1.3",
    "rimraf": "^2.6.3",
    "mini-css-extract-plugin": "^0.5.0",
    "script-ext-html-webpack-plugin": "^2.1.3",
    "style-loader": "^0.23.1",
    "stylelint": "^10.0.1",
    "stylelint-config-recommended": "^2.2.0",
    "svelte-loader": "github:rixo/svelte-loader#hmr",
    "svelte-testing-library": "^1.6.0",
    "svelte-preprocess-sass": "^0.2.0",
    "tailwindcss": "^1.0.3",
    "sass-loader": "^7.1.0",
    "node-sass": "^4.13.1",
    "url-loader": "^1.1.2",
    "webpack": "^4.32.0",
    "webpack-cli": "^3.3.2",
    "webpack-dev-server": "^3.4.1"
  }
}
